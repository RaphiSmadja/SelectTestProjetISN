<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Score">
    <children>
        <ImageView fitHeight="476.0" fitWidth="619.0" layoutX="214.0" layoutY="141.0">
            <image>
                <Image url="@../ressources/bgScore.png" />
            </image>
        </ImageView>
        <Label id="textScore" fx:id="textScore" layoutX="445.0" layoutY="196.0" prefHeight="60.0" prefWidth="158.0" text="Score :" textFill="#526579">
         <font>
            <Font name="Zapftig-Regular" size="36.0" />
         </font></Label>
        <Label fx:id="numberScore" layoutX="436.0" layoutY="298.0" prefHeight="60.0" prefWidth="180.0" textFill="#526579" />
      <Label id="conseilleScore" fx:id="conseilleScore" layoutX="236.0" layoutY="384.0" prefHeight="125.0" prefWidth="577.0" textAlignment="CENTER" textFill="#526579" wrapText="true">
      </Label>
        <Button fx:id="buttonmenu" layoutX="237.0" layoutY="551.0" mnemonicParsing="false" onAction="#pressButtonMenu" prefWidth="180" style="-fx-background-color: #526579;" text="menu" textAlignment="center" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Arial" size="18.0" />
         </font></Button>
        <Button fx:id="buttonquitter" layoutX="625.0" layoutY="551.0" mnemonicParsing="false" onAction="#pressButtonQuitter" prefWidth="180" style="-fx-background-color: #c4ccd9;" text="quitter" textAlignment="center" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Arial" size="18.0" />
         </font></Button>
    </children>
</AnchorPane>
